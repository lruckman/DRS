        "Microsoft.Extensions.Logging": "1.0.0",
        "Microsoft.Extensions.Logging.Console": "1.0.0",
        "Microsoft.Extensions.Logging.Debug": "1.0.0",
        "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.0",
        "Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0",
        "Microsoft.VisualStudio.Web.CodeGeneration.Tools": {
            "version": "1.0.0-preview2-final",
            "type": "build"
        },
        "Microsoft.VisualStudio.Web.CodeGenerators.Mvc": {
            "version": "1.0.0-preview2-final",
            "type": "build"
        },
        "MediatR": "2.1.0",
        "Ghostscript.NET": "1.2.0",
        "Tesseract": "3.0.2",
        "FluentValidation": "6.4.0-beta2",
        "AutoMapper": "5.0.0",
        "StructureMap.Dnx": "0.5.1-rc2-final",
        "MediaTypeMap": "2.1.0",
        "iTextSharp": "5.5.9",
        "itextsharp.pdfa": "5.5.9",
        "itextsharp.xtra": "5.5.9",
        "Microsoft.Extensions.Configuration.Binder": "1.0.0"   },

    "commands": {"ef": "Microsoft.EntityFrameworkCore.Commands"},

    "tools": {
        "Microsoft.AspNetCore.Razor.Tools" : "1.0.0-preview2-final",
        "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview2-final",
        "Microsoft.EntityFrameworkCore.Tools": "1.0.0-preview2-final",
        "Microsoft.Extensions.SecretManager.Tools": "1.0.0-preview2-final",
        "Microsoft.VisualStudio.Web.CodeGeneration.Tools": {
            "version": "1.0.0-preview2-final",
            "imports": [
                "portable-net45+win8"
            ]
        }
    },

    "frameworks": {
        "net461": {
            "frameworkAssemblies": {
                "System.Drawing": "4.0.0.0",
                "System.Web": "4.0.0.0"
            }
        }
    },

    "buildOptions": {
        "emitEntryPoint": true,
        "preserveCompilationContext": true,
        "copyToOutput": {
            "mappings": {
                "./x64/liblept172.dll": { "include": "./wwwroot/app_data/tessdata/x64/liblept172.dll" },
                "./x64/libtesseract304.dll": { "include": "./wwwroot/app_data/tessdata/x64/libtesseract304.dll" },
                "./x86/liblept172.dll": { "include": "./wwwroot/app_data/tessdata/x86/liblept172.dll" },
                "./x86/libtesseract304.dll": { "include": "./wwwroot/app_data/tessdata/x86/libtesseract304.dll" }
            }
        }
    },

    "publishOptions": {
        "include": [
            "wwwroot",
            "Views",
            "Areas/**/Views",
            "appsettings.json",
            "web.config"
        ]
    },
    "scripts": {
        "prepublish": [ "npm install", "bower install", "gulp clean", "gulp min" ],
        "postpublish": [ "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%" ]
    }
}
